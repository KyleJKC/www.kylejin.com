{
  "extends": ["config:base"],
  "timezone": "America/Los_Angeles",
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "pinDigest", "digest", "rollback", "bump"],
      "automerge": true
    }
  ],
  "automergeStrategy": "rebase",
  "schedule": ["before 7am on Sunday"]
}
